
@{
    ViewData["Title"] = "Index";
}

@model IEnumerable<GetAgencyAdminVM>
<div class="card-body p-4">
    <h5 class="card-title fw-semibold mb-4">Recent Transactions</h5>
    <a asp-action="Create" class="btn btn-secondary m-1 float-end mb-2">Create</a>

    <div class="table-responsive w-100">
        <table class="table text-nowrap mb-0 align-middle table-bordered">
            <thead class="text-dark fs-4">
                <tr>
                    <th class="border-bottom-0">
                        <h6 class="fw-semibold mb-0">Id</h6>
                    </th>
                    <th class="border-bottom-0">
                        <h6 class="fw-semibold mb-0">Name</h6>
                    </th>
                    <th class="border-bottom-0">
                        <h6 class="fw-semibold mb-0">Description</h6>
                    </th>
                    <th class="border-bottom-0">
                        <h6 class="fw-semibold mb-0">Image</h6>
                    </th>
                    <th class="border-bottom-0">
                        <h6 class="fw-semibold mb-0">CreatedTime</h6>
                    </th>
                    <th class="border-bottom-0">
                        <h6 class="fw-semibold mb-0">UpdatedTime</h6>
                    </th>
                    <th class="border-bottom-0">
                        <h6 class="fw-semibold mb-0">Action</h6>
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    @foreach(var data in Model)
                    {
                        <th class="border-bottom-0">
                            <h6 class="fw-semibold mb-0">@data.Id</h6>
                        </th>
                        <th class="border-bottom-0">
                            <h6 class="fw-semibold mb-0">@data.Name</h6>
                        </th>
                        <th class="border-bottom-0">
                            <h6 class="fw-semibold mb-0">@data.Description</h6>
                        </th>
                        <th class="border-bottom-0">
                            <h6 class="fw-semibold mb-0">@data.Image</h6>
                        </th>
                        <th class="border-bottom-0">
                            <h6 class="fw-semibold mb-0">@data.CreatedTime</h6>
                        </th>
                        <th class="border-bottom-0">
                            <h6 class="fw-semibold mb-0">@data.UpdatedTime</h6>
                        </th>
                        <th class="border-bottom-0">
                            <button type="button" class="btn btn-success m-1">Success</button>
                            <button type="button" class="btn btn-secondary m-1">Secondary</button>
                        </th>
                    }

                </tr>
               
            </tbody>
        </table>
    </div>
</div>


